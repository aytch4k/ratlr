{
  "name": "ratlr",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered token deployment and analytics platform",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "turbo": "^1.10.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "npm@9.0.0",
  "engines": {
    "node": ">=18.0.0"
  }
}